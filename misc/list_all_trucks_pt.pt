<html>
  <head>
    <title tal:content="template/title">The title</title>
    <meta http-equiv="content-type" content="text/html;charset=utf-8">

    <link href="/++resource++site_resources/all/js/fancybox/jquery.fancybox-1.3.4.css" rel="stylesheet" type="text/css" />
    <script src="/++resource++site_resources/all/js/jquery-1.4.3.min.js" type="text/javascript" ></script>

    <style type=text/css>
      body {
        font-size:9px; 
        font-family: sans-serif;
      }
      table {
        font-size:100%; 
        border:none;
        border-top: solid 1px #ccc;
        border-right: solid 1px #ccc;
      }
      td, th {
        border-left: solid 1px #ccc;
        border-bottom: solid 1px #ccc;
        margin:0;
        padding:5px;
      }
      td {
        white-space: nowrap;
      }
      th {
        background-color: #ddd;   
        vertical-align:top;
      }
      td.number {
        text-align:right;
      }
      tr {
        border:none;
      }
      tr:hover, tr.odd-row:hover {
        background-color: #fff999;
      }
      tr.odd-row {
        background-color: #eee;
      }
      tr a {
        text-decoration: none;
        color: black;
      } 
      tr a:hover {
        text-decoration: underline;
      }
      tr.error {
        color:red;
      }
      div.error {
        border: solid 2px red;
        padding: 10px;
        color: red;
      }
    </style>
  </head>
  <body>
 
    <div class="error" tal:condition="python: len(options['duplicate_numeric_ids']) > 0">
       Duplicate numeric IDs: <span tal:replace="python:options['duplicate_numeric_ids']" />
    </div>
 
    <p><a href="render_trucks_to_dicts">Render everything for pasting into BANDIT project</a></p>

    <p>For fifth-wheel articulated trucks, capacity of first trailer will automatically be split between truck and trailer.</p>
    
    <table border="0" cellpadding="0" cellspacing="0">
      <tr>
        <th>Num. ID</th>
        <th>Truck ID</th>
        <th>name</th>
        <th>type</th>
        <th>power</th>
        <th>buy cost</th>
        <th>intro date</th>
        <th>speed</th>
        <th>truck capacity</th>
        <th>truck num trailers</th>
        <th>trailer capacities</th>
        <th>5th wheel truck cap. fraction</th>
        <th>truck_graphics_file</th>
        <th>trailer_graphics_files</th>
        <th>truck length</th>
        <th>trailer_lengths</th>
        <th>smoke offset</th>
        <th>model life</th>
        <th>vehicle life</th>
        <th>buy menu extra type info</th>
      </tr>
      <tal:render_skus repeat="vehicle_repeat options/all_vehicles">
        <tr tal:define="oddrow repeat/vehicle_repeat/odd" tal:attributes="class python:(('','odd-row')[oddrow]) + ' ' + (('','error')[vehicle_repeat.numeric_ID in options['duplicate_numeric_ids']])">
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/numeric_ID" />
            </a>            
          </td>
          <td>
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/id" />
            </a>            
          </td>
          <td nowrap="true">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/title" />
            </a>            
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_type" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_power" />
            </a>              
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_buy_cost" />
            </a>              
          </td>
          <td>
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_intro_date/year" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_speed" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:condition="python:vehicle_repeat.truck_type != 'GLOBAL_TRUCK_TYPE_FIFTH_WHEEL'" tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_capacity" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:condition="python:vehicle_repeat.truck_type != 'GLOBAL_TRUCK_TYPE_SOLO'"  tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_num_trailers" />
            </a>
          </td>
          <td>
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:condition="python:vehicle_repeat.truck_type != 'GLOBAL_TRUCK_TYPE_SOLO'" tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/trailer_capacities" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:condition="python:vehicle_repeat.truck_type == 'GLOBAL_TRUCK_TYPE_FIFTH_WHEEL'" tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/fifth_wheel_truck_capacity_fraction" />
            </a>
          </td>
          <td>
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_graphics_file" />
            </a>
          </td>
          <td>
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:condition="python:vehicle_repeat.truck_type != 'GLOBAL_TRUCK_TYPE_SOLO'" tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/trailer_graphics_files" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_length" />
            </a>
          </td>
          <td>
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:condition="python:vehicle_repeat.truck_type != 'GLOBAL_TRUCK_TYPE_SOLO'" tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/trailer_lengths" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_smoke_offset" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_model_life" />
            </a>
          </td>
          <td class="number">
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/truck_vehicle_life" />
            </a>
          </td>
          <td>
            <a class="iframe" tal:attributes="href string:${vehicle_repeat/absolute_url}/manage_propertiesForm">
              <span tal:attributes="title vehicle_repeat/id" tal:content="vehicle_repeat/extra_type_info" />
            </a>
          </td>
        </tr>
      </tal:render_skus>
    </table>

<script src="/++resource++site_resources/all/js/fancybox/jquery.mousewheel-3.0.4.pack.js" type="text/javascript"></script>
<script src="/++resource++site_resources/all/js/fancybox/jquery.fancybox-1.3.4.js" type="text/javascript"></script>
<script type="text/javascript">
$('.iframe').fancybox({
	'transitionIn': 'none',
        'transitionOut': 'none',
        'width': 800,
        'height': 600,
        'onClosed': function(){
                //window.location.reload()
        }
});
</script>

  </body>
</html>