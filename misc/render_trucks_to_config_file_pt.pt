<html>
<body>
<tal:build_vehicles repeat="vehicle_repeat options/vehicles">
<pre>
<tal:scope define="vehicle python:options['vehicles'][vehicle_repeat]">
[<span tal:replace="vehicle_repeat" />]
title                               = <span tal:replace="vehicle/title" />
numeric_id                          = <span tal:replace="vehicle/numeric_id" />
truck_graphics_file                 = <span tal:replace="vehicle/truck_graphics_file" />
truck_type                          = <span tal:replace="vehicle/truck_type" />
extra_type_info                     = <span tal:replace="vehicle/extra_type_info" />

model_life                          = <span tal:replace="vehicle/model_life" />
vehicle_life                        = <span tal:replace="vehicle/vehicle_life" />
intro_date                          = <span tal:replace="vehicle/intro_date" />

speed                               = <span tal:replace="vehicle/speed" />
power                               = <span tal:replace="vehicle/power" />
truck_length                        = <span tal:replace="vehicle/truck_length" />

buy_cost                            = <span tal:replace="vehicle/buy_cost" />
run_cost_multiplier                 = <span tal:replace="vehicle/run_cost_multiplier" /> 
truck_capacity                      = <span tal:replace="vehicle/truck_capacity" />

fifth_wheel_truck_capacity_fraction = <span tal:replace="vehicle/fifth_wheel_truck_capacity_fraction" />
smoke_offset                        = <span tal:replace="vehicle/smoke_offset" />

num_trailers                        = <span tal:replace="vehicle/num_trailers" />
trailer_capacities                  = <span tal:replace="vehicle/trailer_capacities" />
trailer_graphics_files              = <span tal:replace="vehicle/trailer_graphics_files" />
trailer_lengths                     = <span tal:replace="vehicle/trailer_lengths" />
</tal:scope>
</pre>
</tal:build_vehicles>
</body>
</html>